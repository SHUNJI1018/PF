<h2>会員情報編集</h2>
<div>
  <%= form_with model: @customer, url:customer_path(current_customer), local: true ,method: :put do |f| %>
     <div>
      <%= f.label :"ニックネーム" %>
      <%= f.text_field :nickname%>
    </div>
    <div>
      <%= f.label :"氏名" %>
      <%= f.text_field :last_name %>
      <%= f.text_field :first_name %>
    </div>
    <div>
      <%= f.label :"フリガナ" %>
      <%= f.text_field :last_kana_name %>
      <%= f.text_field :first_kana_name %>
    </div>
    <div>
      <%= f.label :"メールアドレス" %>
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "col-sm-6 form-control-static" %>
    </div>
    <div>
      <%= f.submit "編集内容を保存"%>
      <%= link_to '退会する', hide_customers_path %>
    </div>
  <% end %>
</div>
<h2>会員情報編集</h2>
<%= form_with model: @customer, url:customer_path(current_customer), local: true ,method: :put do |f| %>
<table>
  <tr>
    <td><%= f.label :"ニックネーム" %></td>
    <td><%= f.text_field :nickname%></td>
  </tr>
  <tr>
    <td><%= f.label :"氏名" %></td>
    <td><%= f.text_field :last_name %></td>
    <td><%= f.text_field :first_name %></td>
  </tr>
  <tr>
    <td><%= f.label :"フリガナ" %></td>
    <td><%= f.text_field :last_kana_name %></td>
    <td><%= f.text_field :first_kana_name %></td>
  </tr>
  <tr>
    <td><%= f.label :"メールアドレス" %></td>
    <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
  </tr>
  <tr>
    <td><%= f.submit "編集内容を保存"%></td>
  </tr>
  <% end %>
</table>

